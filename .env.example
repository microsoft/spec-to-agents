AZURE_OPENAI_API_VERSION=preview
AZURE_AI_PROJECT_ENDPOINT=https://your-resource.services.ai.azure.com/api/projects/your-project
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-5-mini
WEB_SEARCH_MODEL=gpt-4.1-mini

# Bing Search (from Azure AI Foundry connected resources)
BING_CONNECTION_ID=your_bing_connection_id

# Calendar Storage
CALENDAR_STORAGE_PATH=./data/calendars

# MCP Sequential Thinking Tools
MAX_HISTORY_SIZE=1000

# OpenTelemetry Configuration
ENABLE_OTEL=true
ENABLE_SENSITIVE_DATA=true
# OTLP_ENDPOINT=http://localhost:4317
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your_key_from_azure_ai_foundry